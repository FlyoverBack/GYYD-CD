<!--pages/user/cash/cash.wxml-->
<view class="container">
  <view class="list">
    <text>可提现余额（元）</text>
    <text>20,000.00</text>
    <view class="middle">
      <text>提现到微信钱包</text>
      <view class="middle_item"> 
        <input placeholder='每日限额20000元'></input>
        <button>全部</button>
      </view>
    </view>
    <text>提现免手续费，预计3个工作日内到账</text>
    <button class="num">获取验证码</button>
    <text bindtap="cashrecordClick">提现记录</text>
  </view>
</view>
